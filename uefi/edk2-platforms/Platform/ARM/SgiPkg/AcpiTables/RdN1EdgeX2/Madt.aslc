    // Chip - 1 CPUs

    EFI_ACPI_6_2_GICC_STRUCTURE_INIT( // Neoverse-N1-8
      0, 8, GET_MPID(0x01000000ULL, 0x0), EFI_ACPI_6_2_GIC_ENABLED, 23,
